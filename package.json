{
  "name": "reseter.css",
  "version": "1.0.0",
  "description": "Reset All CSS By Browsers With Reseter.css. And Make Web Look Same Across All Browsers",
  "keywords": [
    "Reset.css",
    "Normalize.css",
    "Sanitize.css",
    "Reboot",
    "Css",
    "Sass",
    "Scss",
    "Stylus",
    "less"
  ],
  "homepage": "https://krishdevdb.github.io/reseter.css/",
  "bugs": {
    "url": "https://github.com/krishdevdb/reseter.css/issues",
    "email": "krishdevdb@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Krish",
    "email": "krishdevdb@gmail.com",
    "url": "https://github.com/krishdevdb/"
  },
  "contributors": [],
  "main": "css/reseter.min.css",
  "bin": {},
  "man": [],
  "directories": {
    "src": "src",
    "dist": "dist"
  },
  "repository": "github:krishdevdb/reseter.css",
  "scripts": {
    "yarn:init": "npm install yarn -g --save-dev",
    "sass:build": "yarn sass:compile && yarn pcss && yarn minify",
    "sass:compile": "sass src/sass/reseter.sass css/reseter.css --no-source-map",
    "scss:build": "yarn scss:compile && yarn pcss && yarn minify",
    "scss:compile": "sass src/scss/reseter.scss css/reseter.css --no-source-map",
    "less:build": "yarn less:compile && yarn pcss && yarn minify",
    "less:compile": "lessc src/less/reseter.less css/reseter.css",
    "stylus:build": "yarn stylus:compile && yarn pcss && yarn minify",
    "stylus:compile": "stylus src/stylus/reseter.styl -o css/reseter.css",
    "pcss": "postcss css/reseter.css -r --no-map -u autoprefixer",
    "minify": "cleancss css/reseter.css -o css/reseter.min.css && bundlewatch --config .bundlewatch.config.json"
  },
  "config": {},
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "10.2.3",
    "bundlewatch": "0.3.1",
    "clean-css-cli": "4.3.0",
    "less": "4.1.0",
    "postcss": "8.2.4",
    "postcss-cli": "8.3.1",
    "sass": "1.32.5",
    "stylelint": "13.9.0",
    "stylelint-config-standard": "20.0.0",
    "stylus": "0.54.8",
    "yarn": "1.22.10"
  },
  "peerDependencies": {},
  "bundledDependencies": [],
  "optionalDependencies": {},
  "engines": {
    "node": "~v15.5.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  }
}
